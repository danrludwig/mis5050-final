<h1><span>user list</span></h1>
<form action="/user/list/update" method="POST">
<table>
    
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Is Admin</th>
    </tr>
    </thead>
<tbody>


    <% for (let i=0; i < users.length; i++) { %>
        <tr>
            <td><label for="firstName"><%= users[i].firstName%></label></td>
            <td><label for="lastName"><%= users[i].lastName%></label></td>
            <td><label for="email"><%= users[i].email%></label></td>
            <td><input type="checkbox" id="isAdmin" name="isAdmin" <%= users[i].isAdmin ? "checked" : "" %>></td>
        </tr> 

    <% } %>
   
</tbody>
</table>
<br>
<button type="submit">Update</button>
</form>