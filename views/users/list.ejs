<h1><span>user list</span></h1>
<table>
    
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Is Admin</th>
    </tr>
</thead>
<tbody>

    <% users.forEach((user) => { %>
        <tr>
            <td><%= user.firstName%></td>
            <td><%= user.lastName %></td>
            <td><%= user.email %></td>
            <td><%= user.isAdmin %></td>
        </tr>

    <% }) %>
   
</tbody>
</table>